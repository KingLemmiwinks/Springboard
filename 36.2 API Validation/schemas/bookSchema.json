{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "books"
    ],
    "properties": {
        "books": {
            "type": "array",
            "default": [],
            "title": "The books Schema",
            "items": {
                "type": "object",
                "default": {},
                "title": "A Schema",
                "required": [
                    "isbn",
                    "amazon_url",
                    "author",
                    "language",
                    "pages",
                    "publisher",
                    "title",
                    "year"
                ],
                "properties": {
                    "isbn": {
                        "type": "string",
                        "default": "",
                        "title": "The isbn Schema",
                        "examples": [
                            "02749947"
                        ]
                    },
                    "amazon_url": {
                        "type": "string",
                        "default": "",
                        "title": "The amazon_url Schema",
                        "examples": [
                            "https://www.amazon.com"
                        ]
                    },
                    "author": {
                        "type": "string",
                        "default": "",
                        "title": "The author Schema",
                        "examples": [
                            "David Wallace"
                        ]
                    },
                    "language": {
                        "type": "string",
                        "default": "",
                        "title": "The language Schema",
                        "examples": [
                            "English"
                        ]
                    },
                    "pages": {
                        "type": "integer",
                        "default": 0,
                        "title": "The pages Schema",
                        "examples": [
                            1079
                        ]
                    },
                    "publisher": {
                        "type": "string",
                        "default": "",
                        "title": "The publisher Schema",
                        "examples": [
                            "Back Bay Books"
                        ]
                    },
                    "title": {
                        "type": "string",
                        "default": "",
                        "title": "The title Schema",
                        "examples": [
                            "Infinite Jest"
                        ]
                    },
                    "year": {
                        "type": "integer",
                        "default": 0,
                        "title": "The year Schema",
                        "examples": [
                            2006
                        ]
                    }
                },
                "examples": [{
                    "isbn": "02749947",
                    "amazon_url": "https://www.amazon.com",
                    "author": "David Wallace",
                    "language": "English",
                    "pages": 1079,
                    "publisher": "Back Bay Books",
                    "title": "Infinite Jest",
                    "year": 2006
                }]
            },
            "examples": [
                [{
                    "isbn": "02749947",
                    "amazon_url": "https://www.amazon.com",
                    "author": "David Wallace",
                    "language": "English",
                    "pages": 1079,
                    "publisher": "Back Bay Books",
                    "title": "Infinite Jest",
                    "year": 2006
                }]
            ]
        }
    },
    "examples": [{
        "books": [{
            "isbn": "02749947",
            "amazon_url": "https://www.amazon.com",
            "author": "David Wallace",
            "language": "English",
            "pages": 1079,
            "publisher": "Back Bay Books",
            "title": "Infinite Jest",
            "year": 2006
        }]
    }]
}